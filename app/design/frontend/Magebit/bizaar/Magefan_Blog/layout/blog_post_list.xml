<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>

        <referenceBlock name="preload">
            <block class="Magebit\MagefanBlog\Block\Index"
                   name="blog.post.preload"
                   template="Magefan_Blog::page/head/list/preload.phtml">
            </block>
        </referenceBlock>

        <referenceBlock name="post_list_toolbar" template="post/list/toolbar.phtml">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Magefan_Blog::post/list/toolbar.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>
