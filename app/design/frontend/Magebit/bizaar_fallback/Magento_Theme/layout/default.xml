<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">images/logo.png</argument>
                <argument name="logo_width" xsi:type="number">184</argument>
                <argument name="logo_height" xsi:type="number">40</argument>
                <argument name="logo_alt" xsi:type="string">Bizaar</argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="bizaar_ai" remove="true" />
    </body>
</page>
