<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <section id="contact_us" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1">
            <label>Contact Us</label>
            <tab>bizaar</tab>
            <resource>Magento_Contact::contact</resource>
            <group id="subject" translate="label" showInDefault="1" showInStore="1" showInWebsite="1">
                <label>Subject</label>
                <field id="options" translate="label" showInWebsite="1" showInStore="1" showInDefault="1">
                    <label>Subject Options</label>
                    <frontend_model>Magebit\Contact\Block\Adminhtml\Form\Field\ContactOptions</frontend_model>
                    <backend_model>Magento\Config\Model\Config\Backend\Serialized\ArraySerialized</backend_model>
                </field>
            </group>
        </section>
    </system>
</config>
