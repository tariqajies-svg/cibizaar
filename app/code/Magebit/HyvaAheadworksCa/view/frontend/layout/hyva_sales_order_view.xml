      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="sales.order.buttons">
            <block name="aw-ca.order.buttons.approve" template="Aheadworks_Ca::order/view/button.phtml" cacheable="false">
                <arguments>
                    <argument name="order_button_view_model" xsi:type="object">Aheadworks\Ca\ViewModel\Order\View\Button\Approve</argument>
