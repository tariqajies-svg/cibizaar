            <block name="aw-ca.order.buttons.reject" template="Aheadworks_Ca::order/view/button.phtml" cacheable="false">
                <arguments>
                    <argument name="order_button_view_model" xsi:type="object">Aheadworks\Ca\ViewModel\Order\View\Button\Reject</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
